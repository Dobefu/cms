{
  "name": "cms",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "cd ./api && go build",
    "dev": "concurrently --names \"CMS,API\" -c white.dim,cyan \"next dev --turbopack\" \"cd ./api && air\"",
    "db:start": "docker compose up -d",
    "db:stop": "docker compose down",
    "build": "next build",
    "start": "next start",
    "lint": "next lint && prettier . --check",
    "lint:fix": "next lint && prettier . --write",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest --coverage",
    "sql": "docker compose exec -it db psql db"
  },
  "dependencies": {
    "@iconify/icons-mdi": "^1.2.48",
    "@tanstack/react-query": "^5.90.15",
    "clsx": "^2.1.1",
    "motion": "^12.23.26",
    "next": "16.1.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwind-merge": "^3.4.0",
    "valibot": "1.2.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@iconify/react": "^6.0.2",
    "@tailwindcss/postcss": "^4.1.17",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@testing-library/react": "^16.3.2",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/node": "^25.0.10",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/eslint-plugin": "^1.6.6",
    "@vitest/ui": "3.2.4",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-testing-library": "^7.15.3",
    "jsdom": "^27.0.0",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "vite-tsconfig-paths": "^6.1.0",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@9.11.0+sha512.0a203ffaed5a3f63242cd064c8fb5892366c103e328079318f78062f24ea8c9d50bc6a47aa3567cabefd824d170e78fa2745ed1f16b132e16436146b7688f19b"
